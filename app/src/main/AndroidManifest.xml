<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_icono_app"
        android:label="@string/nombre_app"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MalagaFree"
        tools:targetApi="31">
        <activity
            android:name=".TicketsYNoticias.Noticias"
            android:exported="false" />
        <activity
            android:name=".TicketsYNoticias.Ticket"
            android:exported="false" />
        <activity
            android:name=".TicketsYNoticias.MenuTicketsYNoticias"
            android:exported="false" />
        <activity
            android:name=".Productos.RecyclerProductos"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Productos.MenuProductos"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Productos.BuscarProductos"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Mapa.MostrarZonaRecycler"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Mapa.MenuMapa"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MenuPrincipal"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>